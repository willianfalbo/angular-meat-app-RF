{
  "name": "meat",
  "version": "1.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "npm-run-all --print-label build:dev --parallel watch:** open:app-** ",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "http:dev": "npm run build:dev && cd dist && npm run open:http-dev && python -m http.server 8000",
    "http:prod": "npm run build:prod && cd dist && npm run open:http-prod && python -m http.server 8001",
    "build:dev": "ng build && cd ./backend && tsc",
    "build:prod": "ng build --prod --extract-css=false",
    "build:prod-bh": "ng build --prod --extract-css=false --base-href \"/meat-app/\"",
    "watch:backend": "cd ./backend && tsc -w",
    "watch:json-server": "nodemon --watch ./backend ./backend/dist/server.js",
    "watch:frontend": "ng serve --watch",
    "open:app-backend": "start https://localhost:3001",
    "open:app-frontend": "start http://localhost:4200",
    "open:http-dev": "start http://localhost:8000",
    "open:http-prod": "start http://localhost:8001"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.0",
    "@angular/common": "6.0.0",
    "@angular/compiler": "6.0.0",
    "@angular/core": "6.0.0",
    "@angular/forms": "6.0.0",
    "@angular/platform-browser": "6.0.0",
    "@angular/platform-browser-dynamic": "6.0.0",
    "@angular/platform-server": "6.0.0",
    "@angular/router": "6.0.0",
    "admin-lte": "2.3.11",
    "ajv": "6.0.0",
    "core-js": "2.5.4",
    "font-awesome": "4.7.0",
    "intl": "1.2.5",
    "jquery": "3.1.1",
    "reflect-metadata": "0.1.10",
    "rxjs": "6.0.0",
    "ts-helpers": "1.1.2",
    "web-animations-js": "2.2.5",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular/cli": "6.0.0",
    "@angular/compiler-cli": "6.0.0",
    "@angular/language-service": "6.0.0",
    "@types/express": "4.0.37",
    "@types/jasmine": "2.8.6",
    "@types/jasminewd2": "2.0.3",
    "@types/jsonwebtoken": "7.2.3",
    "@types/node": "8.9.4",
    "codelyzer": "4.2.1",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "4.2.1",
    "json-server": "0.12.0",
    "jsonwebtoken": "7.4.1",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "1.4.2",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "0.2.2",
    "npm-run-all": "^4.1.5",
    "protractor": "5.3.0",
    "ts-node": "5.0.1",
    "tslint": "5.9.1",
    "typescript": "2.7.2",
    "webdriver-manager": "12.0.6"
  }
}
